    "express": "^4.17.3",
    "on-finished": "^2.4.1",
    "prom-client": "^14.0.1",
    "typed-error": "^3.2.1"
  },
  "devDependencies": {
    "@balena/lint": "^6.2.0",
    "@types/chai": "^4.3.0",
    "@types/chai-http": "^4.2.0",
    "@types/mocha": "^9.1.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "husky": "^4.3.8",
    "lint-staged": "^12.3.7",
